;
; Several simple math procedure tests.
;

(define (positive? x) (> x 0))
(define (negative? x) (< x 0))
(define (zero? x) (= x 0))

(define x 10)
(define y -5)
(define z 0)
(define a (+ y y x))
(zero? z)
(zero? a)
(zero? y)
(zero? x)
(positive? z)
(positive? a)
(positive? y)
(positive? x)
(negative? z)
(negative? a)
(negative? y)
(negative? x)
