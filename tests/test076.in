(define zero? (λ (x) (= x 0)))
(define one? (λ (x) (= x 1)))
(define abs (λ (x) (if (<= x 0) (* -1 x) x)))

(define caddr (λ (x) (car (cdr (cdr x)))))

(zero? 0.01)
(zero? 1)
(zero? (- 0.01 0.01))
(zero? 0)
(one? 0)
(one? 1.01)
(one? (- 1.01 0.01))
(one? 1)
(abs -5)
(abs 0)
(abs 5)
(define cadr (λ (x) (car (cdr x))))
(cadr '(+ x y z q *))
(caddr '(+ x y z q *))
