
// Passing array reference to a function to use

int fsum(int[] d) {
    int i;
    int sum = 0;
    for (i=0; i<#d; i++)
        sum = sum + d[i];
    return sum;
}

void main() {
    int i;
    int a[20];
    
    for (i=0; i<20; i++)
        a[i] = i;

    prints("Sum is ");
    printd(fsum(a));
}
